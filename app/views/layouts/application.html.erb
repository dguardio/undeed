<!DOCTYPE html>
<html>
<head>
  <title>Undeed</title>
  <%= stylesheet_link_tag    'application', media: 'all' %>
  <%= javascript_include_tag 'application' %>
  <%= csrf_meta_tags %>
</head>
<body>
	<%= render "layouts/errors" %>
	<% if signed_in? %>
    <%= current_user.email %>
    <%= button_to "Sign Out", root_url, method: :delete %>
  <% else %>
    <%= link_to "Sign In", new_session_url %>
  <% end %>


<%= yield %>

</body>
</html>
